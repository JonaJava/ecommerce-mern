{
    "version":2,
    "builds": [
      {
        "src": "./app.js",
        "use": "@vercel/node"
      },
      {
        "src": "build/**",
        "use": "@vercel/static"
      }
    ],
  
   "routes": [
    {
        "src": "/product",
        "dest": "app.js"
      },
    {
      "src": "/product/(.*)",
      "dest": "app.js"
    },
    {
        "src": "/brand",
        "dest": "app.js"
      },
    {
      "src": "/brand/(.*)",
      "dest": "app.js"
    },
    {
        "src": "/category",
        "dest": "app.js"
      },
    {
      "src": "/category/(.*)",
      "dest": "app.js"
    },
    {
        "src": "/user",
        "dest": "app.js"
      },
    {
      "src": "/user/(.*)",
      "dest": "app.js"
    },
    {
        "src": "/cart",
        "dest": "app.js"
      },
    {
      "src": "/cart/(.*)",
      "dest": "app.js"
    },
    {
        "src": "/orders",
        "dest": "app.js"
      },
    {
      "src": "/orders/(.*)",
      "dest": "app.js"
    },
    {
        "src": "/auth",
        "dest": "app.js"
      },
    {
      "src": "/auth/(.*)",
      "dest": "app.js"
    },
    {
        "src": "/",
        "dest": "build/index.js"
      },
    {
      "src": "/(.*)",
      "dest": "/build/$1",
      "methods":["POST","GET","PATCH","PUT","DELETE","OPTIONS"]
    }
    
  ]
  }