{
  "version": 2,
  "builds": [
    {
      "src": "app.js",
      "use": "@vercel/node"
    },
    {
      "src": "build/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(product|brand|category|user|cart|orders|auth)(.*)",
      "dest": "/app.js"
    },
    {
      "src": "/",
      "dest": "/build/index.html"
    },
    {
      "src": "/static/(.*)",
      "dest": "/build/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/build/$1"
    }
  ]
}
