{
    "version": 2,
    "builds": [
      {
        "src": "./app.js",
        "use": "@vercel/node"
      },
      {
        "src": "build/**",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      
      {
        "src": "/",
        "dest": "build/index.html"
      },
      {
        "src": "/(.*)",
        "dest": "/build/$1",
        "methods": ["POST", "GET", "PATCH", "PUT", "DELETE", "OPTIONS"]
      },
      
      {
        "src": "/(product|brand|category|user|cart|orders|auth)(.*)",
        "dest": "app.js"
      }
    ]
  }
  
