{
    "version": 2,
    "builds": [
      {
        "src": "build/app.js",
        "use": "@vercel/node"
      },
      {
        "src": "build/static/**/*",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/build/app.js"
      },
      {
        "src": "/static/(.*)",
        "dest": "/build/static/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/build/$1"
      }
    ]
  }
  